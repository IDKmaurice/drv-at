{
  "name": "gencestor",
  "version": "0.11.4",
  "description": "Gencestor is a new and dynamic pedigree tool.",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "publish": "electron-builder --win -p always"
  },
  "build": {
    "appId": "com.gencestor.app",
    "productName": "Gencestor",
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico",
      "publish": [
        {
          "provider": "github",
          "owner": "IDKmaurice",
          "repo": "gencestor"
        }
      ]
    },
    "nsis": {
      "runAfterFinish": false,
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "fileAssociations": [
    {
      "name": "Gencestor ATD",
      "description": "Ahnentafel Dokument",
      "ext": [
        "atd"
      ]
    }
  ],
  "repository": "https://github.com/IDKmaurice/gencestor.git",
  "keywords": [
    "Gencestor",
    "Pedigree",
    "Editor",
    "Database"
  ],
  "author": "Maurice Freuw√∂rt",
  "license": "UNLICENSED",
  "dependencies": {
    "animejs": "^3.0.1",
    "electron-log": "^3.0.7",
    "electron-settings": "^3.2.0",
    "electron-tooltip": "^1.1.5",
    "electron-updater": "^4.1.2",
    "glob": "^7.1.4",
    "jquery": "^3.4.1",
    "lodash": "^4.17.15",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "electron": "^4.2.8",
    "electron-builder": "^21.2.0"
  }
}
